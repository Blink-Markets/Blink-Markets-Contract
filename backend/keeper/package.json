{
  "name": "blinkmarket-keeper",
  "version": "1.0.0",
  "description": "Keeper service for Blinkmarket Stork Oracle integration",
  "main": "dist/index.js",
  "scripts": {
    "start": "node dist/index.js",
    "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
    "build": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\""
  },
  "keywords": [
    "sui",
    "oracle",
    "stork",
    "keeper",
    "prediction-market"
  ],
  "author": "Blink Markets",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui.js": "^0.54.1",
    "axios": "^1.6.5",
    "dotenv": "^16.3.1",
    "express": "^5.2.1",
    "ioredis": "^5.3.2",
    "prom-client": "^15.1.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.6",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.6",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}
